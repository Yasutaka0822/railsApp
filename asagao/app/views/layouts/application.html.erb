<!DOCTYPE html>
<html>
<head>
  <meta charset="urf-8">
  <title><%= page_title %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="container">
    <div id="header">
      <%= render "shered/header" %>
    </div>
    <div id="content">
      <%= yield %>
    </div>
    <div id="sidebar">
      <%= render "shared/sidebar" %>
    </div>
    <div id="footer">
      <%= render "shaer/footer"%>
    </div>
  </div>


</body>
</html>
